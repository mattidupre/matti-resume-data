{
  "compilerOptions": {
    "forceConsistentCasingInFileNames": true,
    "target": "ESNext",
    "lib": ["ESNext"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "allowJs": false,
    "sourceMap": true,
    "strict": true,

    "baseUrl": "."
  },
  "include": ["./build.ts"]
}
